albavit = ["а","б","в","г","д","е","ё","ж","з","и","й","к","л","м","н","о","п","р","с","т","у","ф","х","ц","ч","ш","щ","ъ","ы","ь","э","ю","я"]
letter_num = int(input('Введите номер буквы'))
if letter_num <= 33:
    letter_num = letter_num - 1
    print(albavit[letter_num])
else:
    print("Вы ввели слишком большое число. Если что в русском алфавите всего 33 буквы")
